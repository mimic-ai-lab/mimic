{
  "name": "@mimic/cli",
  "version": "0.1.0",
  "description": "Professional CLI for the Mimic agent simulation platform",
  "main": "dist/index.js",
  "bin": {
    "mimic": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "mimic",
    "agent",
    "simulation",
    "typescript",
    "commander"
  ],
  "author": "Mimic Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mimic/mimic.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/mimic/mimic/issues"
  },
  "homepage": "https://github.com/mimic/mimic#readme"
}
